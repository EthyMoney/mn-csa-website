{
  "name": "mn-csa-website",
  "private": true,
  "version": "2.0.0",
  "description": "A simple web app to act as a front-end for the Minnesota CSA Trello board, used by both teams and FRC Nexus to submit support tickets.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node backend/host.js",
    "dev": "nodemon backend/host.js",
    "nuke-labels": "node backend/nuke-labels.js",
    "docker-build": "docker build -t ultimate360/mn-csa-website .",
    "docker-run": "docker run -d --name mn-csa-website -p 3000:3000 -v mn-csa-website:/usr/src/app/config:rw ultimate360/mn-csa-website",
    "docker-push": "docker push ultimate360/mn-csa-website"
  },
  "author": "Logan S. - EthyMoney",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "nodemon": "^3.1.11"
  }
}